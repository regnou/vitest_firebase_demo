{
	"name": "with-svelte--by-dj-ax",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"test": "vitest",
		"emu": "firebase emulators:start --only firestore --project default",
		"emu-kill": "kill-port --port 9099,8080 && firebase emulators:start --only firestore --project default",
		"emu--david": "firebase emulators:start --only auth,firestore,hosting --import=./offline-seed --export-on-exit --project calmban-dev",
		"dev--david": "concurrently npm:firebase vite"
	},
	"devDependencies": {
		"@firebase/rules-unit-testing": "^2.0.2",
		"@poppanator/sveltekit-svg": "^0.3.2",
		"@sveltejs/adapter-vercel": "next",
		"@sveltejs/kit": "next",
		"@tailwindcss/typography": "^0.5.2",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/svelte": "^3.1.3",
		"@testing-library/user-event": "^14.2.0",
		"@types/dotenv-flow": "^3.2.0",
		"@typescript-eslint/eslint-plugin": "^5.27.1",
		"@typescript-eslint/parser": "^5.27.1",
		"@vitest/ui": "^0.14.1",
		"autoprefixer": "^10.4.7",
		"concurrently": "^7.2.1",
		"dotenv-flow": "^3.2.0",
		"eslint": "^8.17.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"firebase": "^9.8.2",
		"firebase-admin": "^10.2.0",
		"firebase-tools": "^11.0.1",
		"highlight.js": "^11.5.1",
		"highlightjs-svelte": "^1.0.6",
		"jsdom": "^19.0.0",
		"markdown-it": "^13.0.1",
		"markdown-it-anchor": "^8.6.4",
		"postcss": "^8.4.14",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"prettier-plugin-tailwindcss": "^0.1.11",
		"svelte": "^3.48.0",
		"svelte-check": "^2.7.2",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.0.24",
		"tslib": "^2.4.0",
		"typescript": "^4.7.3",
		"vite-plugin-markdown": "^2.0.2",
		"vitest": "^0.14.1",
		"vitest-svelte-kit": "^0.0.6"
	},
	"type": "module",
	"engines": {
		"node": "16.x"
	},
	"volta": {
		"node": "16.15.0"
	}
}
